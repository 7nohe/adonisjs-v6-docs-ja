{
  "name": "adonisjs-web-stater-kit",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "export": "vite build && npm run download:sponsors && node --loader=ts-node/esm bin/build.ts",
    "postexport": "copyfiles -u 1 public/* public/**/* dist",
    "download:sponsors": "node --loader=ts-node/esm bin/download_sponsors.ts",
    "serve": "node --loader=ts-node/esm bin/serve.ts",
    "start": "node --loader=ts-node/esm bin/serve.ts",
    "build": "npm run export",
    "dev": "concurrently \"vite\" \"npm run serve\"",
    "test": "node ace test"
  },
  "imports": {
    "#src/*": "./src/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^6.1.3-25",
    "@adonisjs/eslint-config": "^1.1.9",
    "@adonisjs/prettier-config": "^1.1.9",
    "@adonisjs/tsconfig": "^1.1.9",
    "@adonisjs/vite": "^1.0.0",
    "@alpinejs/collapse": "^3.13.3",
    "@alpinejs/persist": "^3.13.3",
    "@dimerapp/content": "^4.1.0",
    "@dimerapp/docs-theme": "^3.0.2",
    "@dimerapp/edge": "^4.0.0",
    "@dimerapp/markdown": "^7.0.3",
    "@docsearch/css": "^3.5.2",
    "@docsearch/js": "^3.5.2",
    "@swc/core": "^1.3.99",
    "@types/node": "^20.9.4",
    "alpinejs": "^3.13.3",
    "collect.js": "^4.36.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "medium-zoom": "^1.1.0",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2",
    "unpoly": "^3.5.2",
    "vite": "^4.5.0"
  },
  "dependencies": {
    "@adonisjs/ally": "^5.0.0-6",
    "@adonisjs/core": "^6.1.5-29",
    "@adonisjs/static": "^1.0.3-2",
    "@radix-ui/colors": "^3.0.0",
    "dayjs": "^1.11.10",
    "edge-uikit": "^1.0.0-1",
    "edge.js": "^6.0.0-10",
    "graphql-request": "^6.1.0",
    "undici": "^5.27.0"
  },
  "eslintConfig": {
